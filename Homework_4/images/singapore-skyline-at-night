	$(function() {
		$("a.like_image_button").click(function() {
			var t = $(this);
			$.post("/photo/like/h", { photoId: "280373", time: "2pm" },
			function(data){
				if (data=='1') {
					var counter = $("#like_counter");
					var current = parseInt(counter.text());
					current += 1;
					counter.text(current.toString());
					//$("a.like_image_button").fadeOut();
					
				}
				$("a.like_image_button").addClass('active');
			});
		});
		$("#actions > .publish").click(function(){
			var ap = $(this);
			$.post("/photo/publish/h", { "photoId": '280373', "op": 1 }, function(data) {
				if (data=='1') { ap.hide(); }
			});
		});
		$("#actions > .unpublish").click(function(){
			var ap = $(this);
			$.post("/photo/publish/h", { "photoId": '280373', "op": 0 }, function(data) {
				if (data=='1') { ap.hide(); }
			});
		});
		$("#actions > .feature").click(function() {
			var ap = $(this);
			$.post("/photo/feature/h", { "photoId": '280373', "op": 1 }, function(data) {
				if (data=='1') { ap.hide(); }
			});
		});
		$("#actions > .unfeature").click(function(){
			var ap = $(this);
			$.post("/photo/feature/h", { "photoId": '280373', "op": 0 }, function(data) {
				if (data=='1') { ap.hide(); }
			});
		});

	});
	
	
